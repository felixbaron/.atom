"*":
  "atom-material-ui":
    colors:
      abaseColor: "#2196f3"
      predefinedColor: "Blue"
    fonts:
      fontSize: 20
    tabs: {}
    ui: {}
  core:
    autoHideMenuBar: true
    automaticallyUpdate: false
    telemetryConsent: "no"
    themes: [
      "atom-material-ui"
      "atom-dark-syntax"
    ]
  "data-explorer": {}
  editor:
    fontSize: 20
  "spell-check":
    excludedScopes: [
      "text.plain.null-grammar"
      "markup.code"
    ]
  terminus:
    core:
      mapTerminalsTo: "File"
      workingDirectory: "Active File"
    style:
      theme: "atom-dark"
    core: {}
  welcome:
    showOnStartup: false
  zentabs:
    maximumOpenedTabs: 2
